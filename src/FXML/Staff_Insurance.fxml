            <TableColumn fx:id="staff_Id" prefWidth="59.0" text="Staff ID" />
            <TableColumn fx:id="name" minWidth="1.0" prefWidth="91.0" text="Name" />
            <TableColumn fx:id="nid" prefWidth="75.0" text="NID" />
            <TableColumn fx:id="phone" prefWidth="75.0" text="Phone Number" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <effect>
            <InnerShadow />
         </effect>
      </TableView>
      <ComboBox fx:id="insurance_state" layoutX="860.0" layoutY="14.0" prefHeight="28.0" prefWidth="235.0" />
   </children>
</AnchorPane>

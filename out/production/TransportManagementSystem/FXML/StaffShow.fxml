<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="540.0" prefWidth="1223.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StaffShow">
   <children>
      <TableView fx:id="staffTableShow" prefHeight="540.0" prefWidth="1223.0" stylesheets="@../style.css">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="staff_id" prefWidth="59.0" text="Staff ID" />
            <TableColumn fx:id="name_staff" minWidth="1.0" prefWidth="91.0" text="Name" />
            <TableColumn fx:id="nid_staff" prefWidth="75.0" text="NID" />
            <TableColumn fx:id="Phone_number" prefWidth="75.0" text="Phone Number" />
            <TableColumn fx:id="garage_id" prefWidth="75.0" text="Garage ID" />
            <TableColumn fx:id="medical_insurance" prefWidth="75.0" text="Medical Insurance" />
         </columns>
      </TableView>
      <VBox prefHeight="111.0" prefWidth="1223.0" spacing="5.0" AnchorPane.topAnchor="540.0">
         <children>
            <JFXButton buttonType="RAISED" prefHeight="50.0" prefWidth="1223.0" style="-fx-background-color: #393e46;" text="ADD" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="40.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Images/add.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXButton buttonType="RAISED" prefHeight="50.0" prefWidth="1223.0" style="-fx-background-color: #393e46;" text="EDIT" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Images/edit.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
         </children></VBox>
   </children>
</AnchorPane>

<AnchorPane prefHeight="641.0" prefWidth="1223.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ShowTransport">
   <children>
      <TableView fx:id="transport_tableView" layoutX="-1.0" layoutY="1.0" prefHeight="540.0" prefWidth="1223.0" stylesheets="@../style.css">
         <columns>
            <TableColumn fx:id="table_Transport_Plate_No" prefWidth="59.0" text="Transport plate No" />
            <TableColumn fx:id="table_ClassName" minWidth="1.0" prefWidth="91.0" text="Class Name" />
            <TableColumn fx:id="table_GarageID" prefWidth="75.0" text="Garage ID" />
            <TableColumn fx:id="table_Insurance" prefWidth="75.0" text="Insurance" />
            <TableColumn fx:id="table_Servicing_Cost" prefWidth="75.0" text="Servicing Cost" />
            <TableColumn fx:id="table_Servicing_Query" prefWidth="75.0" text="Servicing Query" />
            <TableColumn fx:id="table_Capacity" prefWidth="75.0" text="Capacity" />
            <TableColumn fx:id="table_TransportCondition" prefWidth="75.0" text="TransportCondition" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <VBox layoutY="556.0" prefHeight="111.0" prefWidth="1223.0" spacing="5.0" AnchorPane.topAnchor="540.0">

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="667.0" prefWidth="1112.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ShowDriver">
   <children>
      <TableView fx:id="transport_tableView" layoutX="-1.0" layoutY="1.0" prefHeight="553.0" prefWidth="1112.0" stylesheets="@../style.css">
         <columns>
            <TableColumn fx:id="staff_Id" prefWidth="59.0" text="Staff Id" />
            <TableColumn fx:id="nid" minWidth="1.0" prefWidth="91.0" text="NID" />
            <TableColumn fx:id="Medical_insurance" prefWidth="75.0" text="Medical Insurance" />
            <TableColumn fx:id="Name" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="phone_number" prefWidth="75.0" text="Phone Number" />
            <TableColumn fx:id="transport_plate" prefWidth="75.0" text="Transport Plate " />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <VBox layoutY="556.0" prefHeight="111.0" prefWidth="1112.0" spacing="5.0">
         <children>
            <Button fx:id="add_btn" mnemonicParsing="false" prefHeight="52.0" prefWidth="1149.0" style="-fx-background-color: #393e46;" text="ADD" textAlignment="JUSTIFY" textFill="#fffefe">
               <graphic>
                  <ImageView fitHeight="33.0" fitWidth="94.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Images/add.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
            <Button fx:id="edit_btn" mnemonicParsing="false" prefHeight="51.0" prefWidth="1281.0" style="-fx-background-color: #393e46;" text="EDIT" textFill="#fffdfd">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Images/edit.png" />
                     </image>
                  </ImageView>
               </graphic>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>

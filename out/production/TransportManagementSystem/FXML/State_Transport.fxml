<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TransportState">
   <children>
      <TableView fx:id="transportState_tableView" layoutY="10.0" prefHeight="550.0" prefWidth="1243.0" stylesheets="@../style.css" AnchorPane.topAnchor="50.0">
         <columns>
            <TableColumn fx:id="plate_number" prefWidth="59.0" text="Transport Plate Number" />
            <TableColumn fx:id="insurance" minWidth="1.0" prefWidth="91.0" text="Insurance" />
            <TableColumn fx:id="servicing_query" prefWidth="75.0" text="Servicing Query" />
            <TableColumn fx:id="servicing_cost" prefWidth="75.0" text="Servicing Cost" />
            <TableColumn fx:id="condition" prefWidth="75.0" text="Condition" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <effect>
            <InnerShadow />
         </effect>
      </TableView>
   </children>
</AnchorPane>
